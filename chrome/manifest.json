{
    "manifest_version": 2,
    "minimum_chrome_version": "24",
    "name": "Codebox",
    "description": "Powerful, Collaborative Online/Offline cloud IDE",
    "version": "2.4",
    "offline_enabled": true,
    "app": {
        "background": {
            "scripts": [
                "background.js"
            ]
        }
    },
    "permissions": [
        "webview", "storage"
    ],
    "icons": {
        "128": "icons/128.png"
    }
}