{
    "manifest_version": 2,
    "minimum_chrome_version": "24",
    "name": "Codebox",
    "description": "One IDE, All Platform",
    "version": "1",
    "offline_enabled": true,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "webview", "storage"
    ],
    "icons": {
        "128": "static/images/icons/128.png"
    },
    "sandbox": {
      "pages": ["index.html"]
    }
}